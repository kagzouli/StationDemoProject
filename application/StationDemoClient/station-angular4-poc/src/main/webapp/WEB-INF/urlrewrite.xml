<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite

        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"

        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <rule>
	
       <from>^/(.*)$</from>
	   <condition type="request-uri" operator="notequal">(.*(.css|.js|.js.map|.html|.htm|.gif|.ico|.svg|.png|.jpg|.jpeg|.eot|.woff|.woff2|.ttf|.woff).*)$</condition> 
       <to>/index.html</to>
   
   </rule>

</urlrewrite>