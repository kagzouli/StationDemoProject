[
    {
      "name": "station-front",
      "image": "${station_front_image}",
      "portMappings": [
        {
          "containerPort":  ${station_front_container_port},
          "hostPort": ${station_front_host_port},
          "protocol": "tcp"

        }
      ],
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "${region}"
        }
      ],

      "mountPoints": [
        {
           "ContainerPath": "/usr/share/nginx/html/station-angular4-poc/assets/config",
           "SourceVolume": "efs-station-front-data",
           "readOnly": true
        }
      ],
    
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "${awslogs_group}",
          "awslogs-region": "${region}",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
  
